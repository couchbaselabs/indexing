json : "{ " properties "}".

properties  : DQ "name"     DQ ": " DQ projectname DQ ", " NL
              DQ "members"  DQ ": " "[" members "]" ", " NL
              DQ "language" DQ ": " DQ language DQ.

projectname : bag("propernames").

members     : members "," DQ bag("propernames") DQ {100}
            | DQ bag("propernames") DQ.

language    : "python"
            | "haskell"
            | "lua"
            | "golang".
